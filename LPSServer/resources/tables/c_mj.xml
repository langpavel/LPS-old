<table id="c_mj">
	<desc>Měrné jednotky</desc>
	<detail-name>generic</detail-name>
	<list-sql>select * from c_mj</list-sql>
	<edit-sql>select * from c_mj</edit-sql>
	<columns>
		<column name="id" caption="Id" visible="false" editable="true" required="false" width="0" />
		<column name="kod" caption="Kod" visible="true" editable="true" required="false" width="0" />
		<column name="popis" caption="Popis" visible="true" editable="true" required="false" width="0" />
		<column name="popis2" caption="Popis2" visible="true" editable="true" required="false" width="0" />
		<column name="id_user_create" caption="Vytvořil" visible="true" editable="false" required="false" width="0" display="{1}, {2}">
			<ref-table>users</ref-table>
			<replace-columns>surname, first_name</replace-columns>
		</column>
		<column name="dt_create" caption="Vytvořeno" visible="true" editable="false" required="false" width="0" />
		<column name="id_user_modify" caption="Změnil" visible="true" editable="false" required="false" width="0" display="{1}, {2}">
			<ref-table>users</ref-table>
			<replace-columns>surname, first_name</replace-columns>
		</column>
		<column name="dt_modify" caption="Změněno" visible="true" editable="false" required="false" width="0" />
	</columns>
</table>
